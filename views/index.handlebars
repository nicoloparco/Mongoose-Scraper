<div>
    <h1 class="card z-depth-3 center-align" style="font-family: 'Gelasio', serif;">Read the latest from "The New York Times"</h1>
    <div class="container">
        <div class="row">
            <img class="responsive-img col s6" src="https://images.pexels.com/photos/518543/pexels-photo-518543.jpeg?auto=compress&cs=tinysrgb&dpr=1&w=500">
            <img class="responsive-img col s6" src="https://media.istockphoto.com/photos/stack-picture-id881845728?k=6&m=881845728&s=612x612&w=0&h=Q6wnp5nGbO23ui3hcvXQ9QzLY9sdvtdMGe1uonf5Pic=">
        </div>
    </div>
    <div class="row">
        <div class="col s3"></div>
        <h5 class="card z-depth-3 center-align col sm6" style="font-family: 'Gelasio', serif;">Click "Scrape" to get the latest articles</h5>
    </div>  
</div>
<div class="row">
    <div class="col sm2"></div>
    <div class="col s8 left-align">
        {{#each article}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <a href="/readArticle/{{this.id}}" class="waves effect-waves waves-light btn" id="btnSignUp">Read Article</a>
        </div>
        {{/each}}
    </div>
    </div>
</div>  

